{
  "extends": "./tsconfig.base.json",
  "include": [
    "src"
  ],
  "compilerOptions": {
    "types": [
      "@cloudflare/workers-types",
      "node"
    ],
    "outDir": "build/src",
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "rootDir": "src",
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "include": [
      "src",
      "worker-configuration.d.ts"
    ]
  }
}
